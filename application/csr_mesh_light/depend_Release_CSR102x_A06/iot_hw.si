 146:../mesh_common/mesh/drivers/iot_hw.c ****  *
 147:../mesh_common/mesh/drivers/iot_hw.c ****  *  DESCRIPTION
 148:../mesh_common/mesh/drivers/iot_hw.c ****  *      This function sets power state of LED.
 149:../mesh_common/mesh/drivers/iot_hw.c ****  *
 150:../mesh_common/mesh/drivers/iot_hw.c ****  *  RETURNS
 151:../mesh_common/mesh/drivers/iot_hw.c ****  *      Nothing.
 152:../mesh_common/mesh/drivers/iot_hw.c ****  *
 153:../mesh_common/mesh/drivers/iot_hw.c ****  *---------------------------------------------------------------------------*/
 154:../mesh_common/mesh/drivers/iot_hw.c **** extern void IOTLightControlDevicePower(bool power_on)
 155:../mesh_common/mesh/drivers/iot_hw.c **** {
 156:../mesh_common/mesh/drivers/iot_hw.c ****     if (power_on == TRUE)
 157:../mesh_common/mesh/drivers/iot_hw.c ****     {
 158:../mesh_common/mesh/drivers/iot_hw.c ****         if(pio_open == FALSE)
 159:../mesh_common/mesh/drivers/iot_hw.c ****         {
 160:../mesh_common/mesh/drivers/iot_hw.c ****             /* Open the pwm on switch on */
 161:../mesh_common/mesh/drivers/iot_hw.c ****             pwm_ramp_red = PioOpenPWM(LED_PIO_RED, pio_pwm_ramp);
 162:../mesh_common/mesh/drivers/iot_hw.c ****             pwm_ramp_green = PioOpenPWM(LED_PIO_GREEN, pio_pwm_ramp);
 163:../mesh_common/mesh/drivers/iot_hw.c ****             pwm_ramp_blue = PioOpenPWM(LED_PIO_BLUE, pio_pwm_ramp);
 164:../mesh_common/mesh/drivers/iot_hw.c ****             pio_open = TRUE;
 165:../mesh_common/mesh/drivers/iot_hw.c ****         }
 166:../mesh_common/mesh/drivers/iot_hw.c ****         /* Enable PWM channel */
 167:../mesh_common/mesh/drivers/iot_hw.c ****         PioEnablePWM(pwm_ramp_red, TRUE);
 168:../mesh_common/mesh/drivers/iot_hw.c ****         PioEnablePWM(pwm_ramp_green, TRUE);
 169:../mesh_common/mesh/drivers/iot_hw.c ****         PioEnablePWM(pwm_ramp_blue, TRUE);
 170:../mesh_common/mesh/drivers/iot_hw.c ****     }
 171:../mesh_common/mesh/drivers/iot_hw.c ****     else if(pio_open == TRUE)
 172:../mesh_common/mesh/drivers/iot_hw.c ****     {
 173:../mesh_common/mesh/drivers/iot_hw.c ****         /* When power off is selected, disable and close all PWMs */
 174:../mesh_common/mesh/drivers/iot_hw.c ****         PioEnablePWM(pwm_ramp_red, FALSE);
 175:../mesh_common/mesh/drivers/iot_hw.c ****         PioEnablePWM(pwm_ramp_green, FALSE);
 176:../mesh_common/mesh/drivers/iot_hw.c ****         PioEnablePWM(pwm_ramp_blue, FALSE);
 177:../mesh_common/mesh/drivers/iot_hw.c **** 
 178:../mesh_common/mesh/drivers/iot_hw.c ****         PioClosePWM(pwm_ramp_red);
 179:../mesh_common/mesh/drivers/iot_hw.c ****         PioClosePWM(pwm_ramp_green);
 180:../mesh_common/mesh/drivers/iot_hw.c ****         PioClosePWM(pwm_ramp_blue);
 181:../mesh_common/mesh/drivers/iot_hw.c **** 
 182:../mesh_common/mesh/drivers/iot_hw.c ****         PioSet(LED_PIO_RED, 1);
 183:../mesh_common/mesh/drivers/iot_hw.c ****         PioSet(LED_PIO_GREEN, 1);
 184:../mesh_common/mesh/drivers/iot_hw.c ****         PioSet(LED_PIO_BLUE, 1);
 185:../mesh_common/mesh/drivers/iot_hw.c ****         pio_open = FALSE;
 186:../mesh_common/mesh/drivers/iot_hw.c ****     }
 187:../mesh_common/mesh/drivers/iot_hw.c **** }
