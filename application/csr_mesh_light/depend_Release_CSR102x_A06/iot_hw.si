 121:../mesh_common/mesh/drivers/iot_hw.c ****  *---------------------------------------------------------------------------*/
 122:../mesh_common/mesh/drivers/iot_hw.c **** extern void IOTLightControlDeviceInit(void)
 123:../mesh_common/mesh/drivers/iot_hw.c **** {
 124:../mesh_common/mesh/drivers/iot_hw.c ****     /* Configure the LED_PIO_RED PIO as output PIO */
 125:../mesh_common/mesh/drivers/iot_hw.c ****     PioSetDir(LED_PIO_RED, PIO_DIRECTION_OUTPUT);
 126:../mesh_common/mesh/drivers/iot_hw.c **** 
 127:../mesh_common/mesh/drivers/iot_hw.c ****     /* Configure the LED_PIO_GREEN PIO as output PIO */
 128:../mesh_common/mesh/drivers/iot_hw.c ****     PioSetDir(LED_PIO_GREEN, PIO_DIRECTION_OUTPUT);
 129:../mesh_common/mesh/drivers/iot_hw.c **** 
 130:../mesh_common/mesh/drivers/iot_hw.c ****     /* Configure the LED_PIO_BLUE PIO as output PIO */
 131:../mesh_common/mesh/drivers/iot_hw.c ****     PioSetDir(LED_PIO_BLUE, PIO_DIRECTION_OUTPUT);
 132:../mesh_common/mesh/drivers/iot_hw.c **** 
 133:../mesh_common/mesh/drivers/iot_hw.c ****     /* Open a PWM instance */
 134:../mesh_common/mesh/drivers/iot_hw.c ****     pwm_ramp_red = PioOpenPWM(LED_PIO_RED, pio_pwm_ramp);
 135:../mesh_common/mesh/drivers/iot_hw.c ****     pwm_ramp_green = PioOpenPWM(LED_PIO_GREEN, pio_pwm_ramp);
 136:../mesh_common/mesh/drivers/iot_hw.c ****     pwm_ramp_blue = PioOpenPWM(LED_PIO_BLUE, pio_pwm_ramp);
 137:../mesh_common/mesh/drivers/iot_hw.c ****     pio_open = TRUE;
 138:../mesh_common/mesh/drivers/iot_hw.c **** 
 139:../mesh_common/mesh/drivers/iot_hw.c ****     /* Set the light to OFF state on initialization */
 140:../mesh_common/mesh/drivers/iot_hw.c ****     IOTLightControlDevicePower(FALSE);
 141:../mesh_common/mesh/drivers/iot_hw.c **** }
